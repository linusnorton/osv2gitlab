{
  "name": "osv2gitlab",
  "version": "1.0.0",
  "description": "Convert osv-scanner report JSON to GitLab dependency scanner report format",
  "main": "index.js",
  "bin": "index.js",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "start": "node index.js",
    "test": "node index.js ./report.json > output.json",
    "test:validate":  "ajv --strict=false -s schema.json -d output.json",
    "test:all": "npm test && npm run test:validate"
  },
  "private": false,
  "devDependencies": {
    "ajv-cli": "^5.0.0"
  }
}
